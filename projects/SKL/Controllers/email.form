<EntityForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:dummy="0" flexi="0" fleximode="1" fullscreen="0" resize="0" version="2">
  <IsContactForm>false</IsContactForm>
  <EntityName>email</EntityName>
  <CamelCase>Email</CamelCase>
  <PrimaryCommands>!defcmd</PrimaryCommands>
  <SharedVariableNames/>
  <ClassBinding/>
  <EnabledWorkflows>OnLoadEnabled OnChangeEnabled OnSaveEnabled OnExecuteCommand OnCanExecuteCommand</EnabledWorkflows>
  <SerializedChildrenV2>
    <FormTab xsi:type="DetailTab">
      <Name>General</Name>
      <SerializedChildren>
        <DetailItem SchemaName="from" Style="EmailStyle"/>
        <DetailItem SchemaName="to" Style="EmailStyle"/>
        <DetailItem SchemaName="subject" Style="EmailStyle"/>
        <DetailItem separator="true" xsi:type="SeparatorDetailItem">
          <IsConstant>true</IsConstant>
        </DetailItem>
        <DetailItem SchemaName="regardingobjectid" Style="EmailStyle"/>
        <DetailItem SchemaName="description"/>
      </SerializedChildren>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Вложения List</Name>
      <Binding SchemaName="activitymimeattachment.objectid"/>
    </FormTab>
  </SerializedChildrenV2>
</EntityForm>